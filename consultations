                    keyStart: 'site/5438407c6e6f64462d020000/8247c454c347f596a42a8f08ee6c84b6/${filename}',
					callback: function (url, key) {
                      $.ajax({
                            url : "/web_pages/add_upload_from_content_block",
                            type: "POST",
                            data : {authenticity_token: '<%=form_authenticity_token%>',upload:{url:url}},
                            success: function(data, textStatus, jqXHR)
                            {
                                log('saved image to uploads');
                                log(textStatus);
                            },
                            error: function (jqXHR, textStatus, errorThrown)
                            {
                                log('saved image to uploads');
                                log(textStatus);
                            }
                        });
                    },
                    params: {
					  utf8: '&#x2713;',
                      acl: 'public-read', // ACL according to Amazon Documentation.
                      AWSAccessKeyId: 'AKIAJDG7LVTTRXSI4QEA', // Access Key from Amazon.
